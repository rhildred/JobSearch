<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ca.on.conestogac.JobSeeker" table="JobSeekers">
        <id name="sEmailAddress" type="string">
            <column name="EmailAddress" length="255" not-null="true" />
            <generator class="assigned" />
        </id>
        <property name="sFirstName" type="string">
            <column name="FirstName" length="255" not-null="true" />
        </property>
        <property name="sLastName" type="string">
            <column name="LastName" length="255" not-null="true" />
        </property>
    </class>
</hibernate-mapping>